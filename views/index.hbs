    <!-- Begin of career section -->
    {{#unless hideSection.careers}}
      <div
        class="section section-description fp-auto-height-responsive"
        data-section="careers"
      >
        <div class="section-cover-full">
          <div class="cover-container">
            <div class="row gx-0">
              <div class="col-auto col-lg-8 bg-part"></div>
              <div
                class="col-12 col-lg-4 bg-part bg-img"
                data-image-src="img/items/img-portrait-4.jpg"
              ></div>
            </div>
          </div>
        </div>
        <!-- Begin of section wrapper -->
        <div
          class="section-wrapper fullwidth fullheight with-margin center-lg-v"
        >
          <!-- content -->
          <div class="section-content anim">
            <div class="row justify-content-between">
              <div class="col-12 text-left center-v pr-md-5 pr-lg-0">
                <div class="wrapper">
                  <div class="form-desc">
                    <h2 class="display-4 display-title  anim-2">
                      Careers
                    </h2>
                    <p class="invite  anim-3">
                      {{#if sectionIntroduction}}
                        {{{sectionIntroduction.careers}}}
                      {{/if}}
                    </p>
                  </div>
                  <div class="form-input  anim-4">
                    <div class="form-group mb-0">
                      <div class="btns-action anim-3">
                        
                        <a
                          id="current-careers"
                          name="current-careers"
                          class="btn btn-underline btn-primary form-success-gone"
                          {{#if careerPortalLink}}
                          href="{{careerPortalLink.link}}"
                          {{else}}
                          href="#"
                          {{/if}}
                        >
                          <span class="text">
                            Current Openings
                          </span>
                          <span class="icon">
                            <i class="ion ion-checkmark"></i>
                          </span>
                        </a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-12 col-md-6 col-lg-5"></div>
